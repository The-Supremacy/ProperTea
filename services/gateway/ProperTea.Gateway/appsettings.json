{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "Authentication": {
    "External": {
      "Authority": "",
      "Issuer": "",
      "Audience": "propertea-api"
    },
    "Internal": {
      "Issuer": "",
      "Audience": "internal-services",
      "ExpirySeconds": 60,
      "UseDeterministicKeys": false
    }
  },
  "Services": {
    "Authorization": {
      "BaseUrl": ""
    }
  },
  "ReverseProxy": {
    "Routes": {
      "organizations-route": {
        "ClusterId": "organizations",
        "Match": {
          "Path": "/api/v0/organization/{organizationId}/organizations/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "X-Org-Id",
            "Set": "{organizationId}"
          },
          {
            "PathPattern": "/api/v0/organizations/{**catch-all}"
          }
        ]
      },
      "companies-route": {
        "ClusterId": "companies",
        "Match": {
          "Path": "/api/v0/organization/{organizationId}/companies/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "X-Org-Id",
            "Set": "{organizationId}"
          },
          {
            "PathPattern": "/api/v0/companies/{**catch-all}"
          }
        ]
      },
      "property-route": {
        "ClusterId": "property",
        "Match": {
          "Path": "/api/v0/organization/{organizationId}/property/{**catch-all}"
        },
        "Transforms": [
          {
            "RequestHeader": "X-Org-Id",
            "Set": "{organizationId}"
          },
          {
            "PathPattern": "/api/v0/property/{**catch-all}"
          }
        ]
      }
    },
    "Clusters": {
      "organizations": {
        "Destinations": {
          "destination1": {
            "Address": ""
          }
        }
      },
      "companies": {
        "Destinations": {
          "destination1": {
            "Address": ""
          }
        }
      },
      "property": {
        "Destinations": {
          "destination1": {
            "Address": ""
          }
        }
      }
    }
  }
}
